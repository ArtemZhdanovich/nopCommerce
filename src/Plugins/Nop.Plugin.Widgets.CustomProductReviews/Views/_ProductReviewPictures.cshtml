@model List<PictureModel>
@using Nop.Web.Models.Media

@if (Model.Count > 0)
{

    <div class="picture-thumbs" id="main-review-img">
            @foreach (var picture in Model)
            {
            <div class="thumb-item" >
                <img src="@picture.ImageUrl" alt="@picture.AlternateText" title="@picture.Title" data-defaultsize="@picture.ImageUrl" data-fullsize="@picture.FullSizeImageUrl"/>
                    <div class="thumb-item-content">
                        <img src="@picture.FullSizeImageUrl" />
                    </div>
            </div>
            }
    </div>
    
    

}
