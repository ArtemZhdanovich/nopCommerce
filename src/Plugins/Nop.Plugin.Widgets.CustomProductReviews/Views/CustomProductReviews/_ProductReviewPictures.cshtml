@model List<PictureModel>
@using Nop.Web.Models.Media

@if (Model.Count > 0)
{
    <div class="picture-thumbs">
        @foreach (var picture in Model)
        {
            <div class="thumb-item">
                <a href="@picture.FullSizeImageUrl" target="_blank">
                    <img src="@picture.ThumbImageUrl" alt="@picture.AlternateText" title="@picture.Title" data-defaultsize="@picture.ImageUrl"/>
                </a>
            </div>
        }
    </div>
}
