@model Nop.Plugin.Admin.Accounting.Models.AccountingConfigureModel
@{
    Layout = "_ConfigurePlugin";
}
@inject Nop.Core.IWebHelper webHelper

<form asp-controller="Accounting" asp-action="Configure" method="post">
    <div class="panel-group">
        <div class="panel panel-default">
            <div class="panel-body">
                <h4>Indstillinger for bogholderi</h4>
                <hr />

                <h4>Kaching</h4>
                <br />
                <div class="form-group row">
                    <div class="col-md-3">
                        <nop-label asp-for="ReconciliationListDaysBack" />
                    </div>
                    <div class="col-md-9">
                        <nop-editor asp-for="ReconciliationListDaysBack" />
                        <span asp-validation-for="ReconciliationListDaysBack"></span>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-3"></div>
                    <div class="col-md-9">
                        <span>Dage listen over kasselukninger går tilbage</span>
                    </div>
                </div>

                <hr />

                <h4>
                    Product numbers found in e-conomic
                </h4>
                <div class="form-group row">
                    <div class="col-md-3">
                        <nop-label asp-for="PhysicalShop_ProductNumber" />
                    </div>
                    <div class="col-md-9">
                        <nop-editor asp-for="PhysicalShop_ProductNumber" />
                        <span asp-validation-for="PhysicalShop_ProductNumber"></span>
                    </div>
                </div>
                 <div class="form-group row">
                    <div class="col-md-3">
                        <nop-label asp-for="WebshopDK_ProductNumber" />
                    </div>
                    <div class="col-md-9">
                        <nop-editor asp-for="WebshopDK_ProductNumber" />
                        <span asp-validation-for="WebshopDK_ProductNumber"></span>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-3">
                        <nop-label asp-for="WebshopSE_ProductNumber" />
                    </div>
                    <div class="col-md-9">
                        <nop-editor asp-for="WebshopSE_ProductNumber" />
                        <span asp-validation-for="WebshopSE_ProductNumber"></span>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-3">
                        <nop-label asp-for="WebshopOther_ProductNumber" />
                    </div>
                    <div class="col-md-9">
                        <nop-editor asp-for="WebshopOther_ProductNumber" />
                        <span asp-validation-for="WebshopOther_ProductNumber"></span>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-3">
                        <nop-label asp-for="WebshopOtherWithoutTax_ProductNumber" />
                    </div>
                    <div class="col-md-9">
                        <nop-editor asp-for="WebshopOtherWithoutTax_ProductNumber" />
                        <span asp-validation-for="WebshopOtherWithoutTax_ProductNumber"></span>
                    </div>
                </div>

                <hr />
                <div class="form-group">
                    <div class="col-md-9 col-md-offset-3">
                        <input type="submit" name="save" class="btn bg-primary" value="@T("Admin.Common.Save")" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
<div class="field-validation-error">
    <h4>@Model.ErrorMessage</h4>
</div>
      