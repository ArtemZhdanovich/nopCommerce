        _ltk.SCA.CaptureEmail('Email');
        _ltk.SCA.CaptureEmail('newsletter-email');
        _ltk.SCA.CaptureEmail('BillingNewAddress_Email');
        _ltk.SCA.CaptureEmail('ShippingNewAddress_Email');
    });
</script>